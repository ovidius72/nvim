{
  "typescript.suggest.autoImports": true,
  "typescript.suggest.enabled": true,
  "tsserver.formatOnType": true,
  "emmet.excludeLanguages": ["markdown", "typescriptreact"],
  "tsserver.enable": true,
  "suggest.echodocSupport": true,
  "coc.source.file.priority": 0,
  "project.rootPatterns": [
    ".git",
    "node_modules"
  ],
  "git.addGBlameToBufferVar": true,
  "smartf.timeout": 4000,
  "smartf.wordJump": false,
  "smartf.jumpOnTrigger": false,
  "coc.source.around.priority": 1,
  "emmet.priority": 3,
  "coc.source.buffer.priority": 0,
  "suggest.languageSourcePriority": 99,
  "codeLens.enable": false,
  "typescript.showUnused": true,
  "javascript.suggest.autoImports": true,
  "python.jediEnabled": false,
  "signature.enable": true,
  "diagnostic.virtualText": true,
  "git.virtualTextPrefix": "Ôíì ",
  "diagnostic.virtualTextPrefix": "[ ÔÅ™ ] ",
  "diagnostic.virtualTextCurrentLineOnly": false,
  "git.addGBlameToVirtualText": false,
  "signature.preferShownAbove": true,
  "signature.hideOnTextChange": true,
  "java.signatureHelp.enabled": true,
  "signature.triggerSignatureWait": 300,
  "signature.target": "float",
  "diagnostic.enable": true,
  "diagnostic.infoSign": "ÔöÉ",
  "diagnostic.errorSign": "Ôñ¨",
  "diagnostic.warningSign": "Ôî•",
  "diagnostic.hintSign": "Ôúá",
  "diagnostic.checkCurrentLine": true,
  "diagnostic.enableHighlightLineNumber": true,
  "diagnostic.level": "hint",
  "diagnostic.enableMessage": "always",
  "diagnostic.displayByAle": false,
  "yaml.completion": false,
  "yank.priority": 0,
  "yank.enableCompletion": false,
  "git.enableGlobalStatus": true,
  "git.signOffset": 1,
  "git.enableGutters": true,
  "explorer.buffer.showHiddenBuffers": false,
  "explorer.icon.enableNerdfont": true,
  "explorer.openAction.select.filterFloatWindows": false,
  "explorer.file.column.git.icon.added": "‚úö",
  "explorer.file.column.git.icon.untracked": "‚óé",
  "explorer.file.column.git.icon.modified": "‚úê",
  "explorer.previewAction.onHover": false,
  "explorer.file.column.git.icon.deleted": "üó∑",
  "explorer.file.column.git.icon.mixed": "‚Åâ",
  "explorer.file.column.git.icon.copied": "‚ûú",
  "explorer.file.column.git.icon.renamed": "¬Æ",
  "explorer.file.column.git.icon.unmerged": "üóà",
  "explorer.file.showHiddenFiles": true,
  "snippets.shortcut": "S",
  "snippets.ultisnips.directories": [ "~/.config/nvim/ultisnips", "UltiSnips" ],
  "snippets.userSnippetsDirectory": "~/.config/nvim/ultisnips",
  "snippets.extends": {
    "cpp": ["c"],
    "php": ["php", "module", "install", "inc"],
    "python": ["py", "html"],
    "javascript": ["javascript"],
    "javascriptreact": ["javascript"],
    "typescript": ["javascript"],
    "typescriptreact": ["typescript", "javascript"]
  },
  "languageserver": {
    "ccls": {
      "command": "ccls",
      "filetypes": ["c", "cpp", "objc", "objcpp"],
      "rootPatterns": [
        ".ccls",
        "compile_commands.json",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "initializationOptions": {
        "cache": {
          "directory": "/tmp/ccls"
        }
      }
    },
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"],
      "ignoredRootPaths": ["~"]
    },
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": ["dockerfile"],
      "args": ["--stdio"]
    },
    "intelephense": {
      "command": "intelephense",
      "args": ["--stdio"],
      "filetypes": ["php", ".module", ".install", ".inc", "inc", "module"],
      "progressOnInitialization": true,
      "settings": {
        "intelephense": {
          "files": {
            "maxSize": 3000000,
            "associations": [
              "*.php",
              "*.phtml",
              "*.inc",
              "*.module",
              "*.install",
              "*.theme",
              ".engine",
              ".profile",
              ".info",
              ".test"
            ]
          }
        }
      },
      "initializationOptions": {
        "storagePath": "/tmp/intelephense"
      }
    },
    "elmLS": {
      "command": "elm-language-server",
      "filetypes": ["elm"],
      "rootPatterns": ["elm.json"],
      "initializationOptions": {
        "elmPath": "elm",
        "elmFormatPath": "elm-format",
        "elmTestPath": "elm-test",
        "elmAnalyseTrigger": "change"
      }
    }
  },
  "python.autoComplete.extraPaths": [
    "${env:SPARK_HOME}/python",
    "${env:SPARK_HOME}/python/pyspark"
  ]
}
